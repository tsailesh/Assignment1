package assignment1;

public class DisplayInConsole {
	public static void main(String[] args) {
		System.out.println("<!DOCTYPE html>\n<html lang=\"en-US\">\n");
		System.out.println("<script src=\"http://ajax.googleapis.com/ajax/libs/angularjs/1.3.14/angular.min.js\"></script>");
		System.out.println("<body>");
		System.out.println("<div ng-app=\"\">");
		System.out.println("	<p>Name : <input type=\"text\" ng-model=\"name\"></p>");
		System.out.println("</div>");
		System.out.println("	<h1>Hello '{{name}}'</h1>");
		System.out.println("</body>");
		System.out.println("</html>");

	}

}
