package assignment1;

public class CommandLineSumAverage {
	public static void main(String[] args) {
		
		int sum=0;
		double average=0;
		for (int i = 0; i < args.length; i++) 
			sum=sum+Integer.parseInt(args[i]);
			average=sum/args.length;
			System.out.println("Sum is : "+sum+"\nAverage is : "+average);
		}
}
